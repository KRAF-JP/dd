{
    "name": "krafjp/dd",
    "description": "MySQL/MariaDB database definition document generator for laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "shinji.oda",
            "email": "oda@kraf.jp"
        }
    ],
    "require": {
        "php": "^7.4|^8.1",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "doctrine/dbal": "^2.0 || ^3.0",
        "league/html-to-markdown": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Krafjp\\Dd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Krafjp\\Dd\\DdServiceProvider"
            ]
        }
    }
}
